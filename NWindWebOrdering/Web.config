<configuration>
  <appSettings>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None"/>
  </appSettings>



  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />


    <authentication mode="Forms">
      <forms loginUrl="Login.aspx" name=".ASPXFORMSAUTH" defaultUrl="AccountManager.aspx" timeout="25">

      </forms>

    </authentication>

    <authorization>
      <deny users="?"/>
    </authorization>

  </system.web>

</configuration>
